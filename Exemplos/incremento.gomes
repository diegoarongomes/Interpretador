//Programa escrito em linguagem Gomes;
//Teste de incremento e decremento;

Var:a;
Imprime([Digite numero menor que 25.]);
Digite(a);
Imprime([Atual: ],a);
Enquanto(a<50);
	a++;
	Imprime([Incrementado: ],a);
	Inicio.se(a==30);
		Quebra.laco;
	Fim.se;
Fim.enquanto;
Enquanto(a>0);
	a--;
	Imprime([Decrementado: ],a);
	Inicio.se(a==5);
		Quebra.laco;
	Fim.se;
Fim.enquanto;
Fim.do.programa;
