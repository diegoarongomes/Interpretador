//Programa escrito em linguagem Gomes
//teste de aninhamento
Var:a;
Var:b;
a=0;
b=3;
Enquanto(a<=10);
	Imprime([Entrei no primeiro]);
	Enquanto(b<=3);
		Imprime([Entrei no segundo]);
		b++;
		Inicio.se(b==4);
			Imprime([Estou no IF]);
		Fim.se;
	Fim.enquanto;
	a++;
	Inicio.se(a > 5);
		Imprime(a);
	Fim.se;
Fim.enquanto;
Imprime([Consegui sair. Deu certo]);
Fim.do.programa;
