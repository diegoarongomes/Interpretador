//Programa escrito em linguagem Gomes
//teste de aninhamento
Var:a;
Var:b;
a=2;
b=3;
Enquanto(a<=2);
	Imprime([Entrei no primeiro]);
	Enquanto(b<=3);
		Imprime([Entrei no segundo]);
		b++;
		Inicio.se(b==4);
			Imprime([Estou no IF]);
		Fim.se;
	Fim.enquanto;
	a++;
Fim.enquanto;
Imprime([Consegui sair. Deu certo]);
Fim.do.programa;
